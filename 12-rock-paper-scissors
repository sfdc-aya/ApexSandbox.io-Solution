public Integer rockPaperScissors(String player1, String player2) {
    if(player1 == 'rock' && player2 == 'paper' || player2 == 'rock' && player1 == 'paper'){
        return 2;
    }else if(player1 == 'scissors' && player2 == 'paper' || player2 == 'scissors' && player1 == 'paper'){
        return 1;
    }else{
        return 0;
    }
}