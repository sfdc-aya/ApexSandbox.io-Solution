public List<apxio__Student__c> selectStudentsWithoutContactInfo() {
    List<apxio__Student__c> stuList = 
                        [Select Id, Name 
                        from apxio__Student__c 
                        where apxio__Active__c = true 
                        AND (apxio__Email__c = null 
                        or apxio__Phone__c = null)  
                        order by Name];
    return stuList;
}